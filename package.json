{
  "name": "mugi-ui",
  "version": "0.0.1",
  "repository": "https://github.com/ushironoko/mugi-ui.git",
  "author": "shigehito",
  "scripts": {
    "dev": "vite",
    "tsc": "vue-tsc --noEmit",
    "build": "npm run tsc && npm run test && vite build",
    "serve": "vite preview",
    "test": "jest"
  },
  "types": "src/index.ts",
  "module": "./dist/mugi-ui.es.js",
  "main": "./dist/mugi-ui.umd.js",
  "files": [
    "dist",
    "src/index.ts",
    "src/components/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/mugi-ui.es.js",
      "require": "./dist/mugi-ui.umd.js"
    },
    "./style": "./dist/style.css"
  },
  "peerDependencies": {
    "vue": "^3.0.5",
    "@vue/compiler-sfc": "^3.0.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "vue": "^3.0.5",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "2.0.0-rc.6",
    "autoprefixer": "^10.3.1",
    "babel-jest": "^26.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.14.0",
    "jest": "^26.0.0",
    "msw": "^0.33.0",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.3.2",
    "sass": "^1.36.0",
    "tailwindcss": "^2.2.6",
    "tailwindcss-pseudo-elements": "^2.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.4.3",
    "vite-plugin-fonts": "^0.2.2",
    "vue-jest": "5.0.0-alpha.10",
    "vue-tsc": "0.0.24"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
